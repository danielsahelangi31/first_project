            if (res.izin_operasi_file!=null){
             $('#izin_operasi_file').html(`<a target="_blank" href="/download-file/${res.izin_operasi_file}">${res.izin_operasi_file}</a> `)
           }

            if (res.bast_file!=null){
                        $('#bast_file').html(`<a target="_blank" href="/download-file/${res.bast_file}">${res.bast_file}</a> `)
                      }

        })
        function clearApproval(){

          $('#izin_operasi_file').html(``)
