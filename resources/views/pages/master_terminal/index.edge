            $('#jumlah_tambat').val(res.jumlah_tambat)
            $('#kedalaman_max').val(res.kedalaman_max)
            $('#kedalaman_min').val(res.kedalaman_min)

            $('#lng').val(res.lng)
            $('#lat').val(res.lat)
