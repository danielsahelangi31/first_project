<script>
    // autominus kedalaman minimal & maximal
    $(".kedalaman").keyup(function() {
      $(this).val($(this).val().replace(/[A-z!@#$%\^&*\(\)_\-+=\{\}\[\]|\\:;<>,?/]/g,""));
      if($(this).val().length >= 1) {
        $(this).val("-" + $(this).val());
      }
    });

    // validasi min & max kedalaman
    $(".kedalaman").change(function() {
      if($("#kedalaman_min").val() != "" && $("#kedalaman_max").val() != "") {
        if(parseFloat($("#kedalaman_min").val()) < parseFloat($("#kedalaman_max").val())) {
          Swal.fire({
            title: "<span style='color:#FF0000;'>Error</span>", 
            html: "<strong>Kedalaman minimal harus lebih besar dari kedalaman maximal</strong>",
            icon: 'error',
            confirmButtonText: "Baik",
            confirmButtonColor: "#045299",
            allowOutsideClick: false
          })
        }
      }
    });

   $("#pengelola").change(function(){
       var idGroup = $(this).val();
       $.ajax({
          type: "GET",
          dataType: "html",
          url: "{{route('select2.companyByGroup')}}",
          data: "group_id="+idGroup,
          success: function(msg){
            $("#company_id").empty();
            $.each(JSON.parse(msg ), function( index, value ) {
              $("#company_id").append('<option value="'+value.id+'">'+value.company_name+'</option>');
            });
          }
       });
     });


    $("#branch_id").change(function(){
     var name = $("#branch_id option:selected").attr("data-name")
     var code = $("#branch_id option:selected").attr("data-code")
     $('#name_sub_wil').val(name)
     $('#code_sub_wil').val(code.substring(2, 5))

   })
  $("#createSubwilAction").click(function(){

    $("#country_code").prop('disabled', true);
    $("#createSubwilAction").hide();
    $('#select-sub-wil').hide();

    $("#selectSubWilAction").show();
    $('#create-sub-wil').show();
    $("#country_code").val('').trigger('change');
    $('#name_sub_wil').val('')
    $('#code_sub_wil').val('')
  });


  $("#selectSubWilAction").click(function(){
    $("#country_code").prop('disabled', false);
    $("#createSubwilAction").show();
    $('#select-sub-wil').show();

    $("#selectSubWilAction").hide();
    $('#create-sub-wil').hide();
    $("#country_code").val('').trigger('change');
    $('#name_sub_wil').val('')
    $('#code_sub_wil').val('')
  });

  $("#country_code").change(function(){
      var value = $(this).val();
      var valueSplit= value.split("|");
      $('#name_sub_wil').val(valueSplit[1])
      $('#code_sub_wil').val(valueSplit[0])
     });
  $('#country_code').select2({
    placeholder: "Pilih Kode Pelabuhan",
    allowClear: true,
    ajax: {
      url: "{{route('select2.cityCode')}}",
      dataType: 'json',
      type: "GET",
      data: function (params) {
        var query = {
          search: params.term,
        }
        // Query parameters will be ?search=[term]&page=[page]
        return query;
      },
      processResults: function (data) {
        return {
          results: $.map(data, function (item) {
            return {
              text: item.city_name + ' (' +   item.city_code.substring(2, 5)+ ')',
              id: item.city_code.substring(2, 5)+'|'+item.city_name
            }
          })
        };
      }
    }
  });


  function saveDraft(){

const form = document.getElementById('form-data');
let data = new FormData(form)
data.append('save_draft', '1');
swallRequest(form.getAttribute('action'), data, '', '', '{{route('master.terminal.index')}}?status=REQUEST')
}

function saveSubmit(){

const form = document.getElementById('form-data');
let data = new FormData(form)
data.append('save_submit', '1');
swallRequest(form.getAttribute('action'), data, '', '', '{{route('master.terminal.index')}}?status=REQUEST')
}
</script>
