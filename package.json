{
  "name": "mdm-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.8.1",
    "@japa/preset-adonis": "^1.1.1",
    "@japa/runner": "^2.1.1",
    "@symfony/webpack-encore": "^3.1.0",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "pino-pretty": "^9.1.0",
    "prettier": "^2.7.1",
    "typescript": "~4.6",
    "youch": "^3.2.1",
    "youch-terminal": "^2.1.5"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.2.3",
    "@adonisjs/bouncer": "^2.3.0",
    "@adonisjs/core": "^5.8.0",
    "@adonisjs/drive": "^2.3.0",
    "@adonisjs/drive-s3": "^1.3.2",
    "@adonisjs/i18n": "^1.5.6",
    "@adonisjs/lucid": "^18.1.1",
    "@adonisjs/mail": "^8.1.2",
    "@adonisjs/repl": "^3.1.0",
    "@adonisjs/session": "^6.2.0",
    "@adonisjs/shield": "^7.1.0",
    "@adonisjs/view": "^6.1.0",
    "@aws-sdk/client-s3": "^3.332.0",
    "argon2": "^0.30.2",
    "aws-sdk": "^2.1377.0",
    "axios": "^1.2.1",
    "exceljs": "^4.3.0",
    "jsontoxml": "^1.0.1",
    "luxon": "^3.2.1",
    "mysql2": "^2.3.3",
    "oracledb": "^5.5.0",
    "phc-argon2": "^1.1.4",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.5.3",
    "source-map-support": "^0.5.21",
    "sweetalert2": "^11.4.8",
    "uuid": "^9.0.0",
    "xml2js": "^0.4.23"
  }
}
